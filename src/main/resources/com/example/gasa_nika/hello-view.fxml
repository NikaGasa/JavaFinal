<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.gasa_nika.MarketController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <TextField fx:id="textField1" promptText="Enter value 1"/>
    <TextField fx:id="textField2" promptText="Enter value 2"/>
    <TextField fx:id="textField3" promptText="Enter value 3"/>
    <TextField fx:id="textField4" promptText="Enter value 4"/>

    <Button text="Hello!" onAction="#addButtonClick"/>
</VBox>
